{
    "order": 0,
    "index_patterns": [
        "i_fpc-metric-dscp-data-record-*"
    ],
    "settings": {
        "index": {
            "number_of_shards": "1",
            "number_of_replicas": "0",
            "refresh_interval": "1s"
        }
    },
    "mappings": {
        "dynamic": false,
        "properties": {
            "timestamp": {
                "type": "date",
                "index": true,
                "doc_values": true
            },
            "network_id": {
                "type": "keyword",
                "index": true,
                "doc_values": true
            },
            "service_id": {
                "type": "keyword",
                "index": true,
                "doc_values": true
            },
            "type":{
                "type": "keyword",
                "index": true,
                "doc_values": true
            },
            "total_bytes": {
                "type": "long",
                "index": false,
                "doc_values": true
            },
			"total_packets": {
                "type": "long",
                "index": false,
                "doc_values": true
            }
        }
    }
}