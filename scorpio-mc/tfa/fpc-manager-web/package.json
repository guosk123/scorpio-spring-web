{
  "name": "tfa-next",
  "version": "4.3.0",
  "private": true,
  "description": "",
  "scripts": {
    "analyze": "cross-env UMI_ENV=prod ANALYZE=1 umi build",
    "build": "cross-env UMI_ENV=prod WEBPACK_FS_CACHE=none umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src/pages && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src/pages",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\"",
    "site": "npm run fetch:blocks && npm run build",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev UMI_UI=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env REACT_APP_ENV=dev UMI_UI=none MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "tsc": "tsc --noEmit",
    "check-config": "cross-env NODE_ENV=development umi webpack",
    "check-plugin": "cross-env NODE_ENV=production umi plugin list --key"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
