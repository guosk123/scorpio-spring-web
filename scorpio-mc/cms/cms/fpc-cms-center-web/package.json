{
  "name": "ant-design-pro",
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env UMI_ENV=prod WEBPACK_FS_CACHE=none umi build",
    "dev": "npm run start:dev",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "serve": "umi-serve",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.0.7",
    "@ant-design/compatible": "^1.0.8",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/plots": "^1.0.9",
    "@ant-design/pro-descriptions": "^1.9.25",
    "@ant-design/pro-form": "^1.58.0",
    "@ant-design/pro-layout": "^6.34.3",
    "@ant-design/pro-list": "^1.21.39",
    "@ant-design/pro-table": "^2.67.0",
    "@antv/x6": "^1.25.0",
    "@antv/x6-react-components": "^1.1.13",
    "@antv/x6-react-shape": "^1.4.5",
    "@types/d3": "^7.4.0",
    "@umijs/route-utils": "^1.0.37",
    "ahooks": "^3.7.2",
    "antd": "^4.18.7",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.2.6",
    "d3": "^7.4.4",
    "d3-scale": "^3.3.0",
    "dva-model-extend": "^0.1.2",
    "echarts": "^5.2.1",
    "echarts-for-react": "^3.0.1",
    "github-markdown-css": "^4.0.0",
    "ip-address": "^7.1.0",
    "jquery": "^3.6.0",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.33",
    "numeral": "^2.0.6",
    "omit.js": "^2.0.2",
    "qs": "^6.10.1",
    "rc-menu": "^9.0.13",
    "rc-util": "^5.18.1",
    "react": "^17.0.0",
    "react-base-table": "^1.12.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-drag-listview": "^0.1.8",
    "react-grid-layout": "^1.3.4",
    "react-helmet-async": "^1.0.4",
    "react-markdown": "4.3.1",
    "react-resizable": "^1.11.1",
    "react-sizeme": "^3.0.2",
    "react-split-pane": "^2.0.3",
    "react-tiny-virtual-list": "^2.2.0",
    "save-svg-as-png": "^1.4.17",
    "umi": "^3.5.8",
    "umi-serve": "^1.9.10",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.1",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-search": "^0.8.2",
    "xterm-addon-web-links": "^0.5.1"
  },
  "devDependencies": {
